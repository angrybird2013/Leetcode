class Solution:
    # @return a list of lists of integers
    def generate(self, numRows):
        r = []
        for i in range(numRows):
            if i > 0:
                a = r[i-1] + [1]
            else:
                a = [1]
            for j in range(1, i):
                a[i-j] = a[i-j] + a[i-j-1]
            r += [a]
        return r
